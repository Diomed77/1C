Процедура ПечатьПоОборотам()
	начЛог = семЗаписатьЛогНач( "Отчет", "Карточка товара", "Обработка", "Формирование отчета" );
	заг="---";    
	ИтогоПрих=0;
	итогоРасх=0;
	ЧислоСтрок=0;
	
	ДатаНачала=ДатаОтчета;

	Таб=СоздатьОбъект("Таблица");   
