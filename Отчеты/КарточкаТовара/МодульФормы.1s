					Исключение
					КонецПопытки;

					Если (ВидДок="ПриходнаяНал") ИЛИ (ВидДок="ПриходнаяКредит") ИЛИ
				       	(ВидДок="ПриходнаяРеализ") Тогда
				       	ПечПриращСтоим=" ";  
				    КонецЕсли;
                КонецЕсли;
    //        		Прервать;
            КонецЕсли;
        КонецЦикла;  

		Если ВидДок="ВедомостьИнвентаризации" Тогда   
			    КолДок=Приращение;
				
				Попытка
	        		ЦенаБаз=ПриращСтоим/Приращение;
				Исключение
					ЦенаБаз=0;
				КонецПопытки;
				
	        	СуммаБаз=ПриращСтоим;
	        	ЦенаДок=0; 
	        	СуммаДок=0; 
	        	Вал=" ";
		        ПечПриращСтоим=" ";
	       	Таб.ВывестиСекцию("Документ");
			Оживить(1);
				
		ИначеЕсли ВидДок="ПродажаЧерезКассу" Тогда
        	Если КолПрод>0 Тогда
        		//Продажа 
        		ЦенаДок=0;
        		СуммаДок=0;
        		КолДок=КолПрод;
        		СуммаБаз=СумПрод;
        		ЦенаБаз=СумПрод/КолПрод; 
                ПриращСтоим=Запрос.РасхСто;
				ПечПриращСтоим=ФРМ(ПриращСтоим,ВБ,1);
				ПечКл="Pardoєana"; 
				Если (ПустоеЗначение(ВыбПризнакНакладной)=1) ИЛИ (ВыбПризнакНакладной=Перечисление.ПризнРасхНакл.Продажа) Тогда
       				Таб.ВывестиСекцию("Документ");
					Оживить(1); 
				КонецЕсли;
			КонецЕсли;
			
			Если КолВозвр>0 Тогда
				// Возврат
        		ЦенаДок=0;
        		СуммаДок=0;
        		КолДок=КолВозвр;
        		СуммаБаз=СумВозвр;
