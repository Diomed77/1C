				ВалютаДок=Док.Валюта;
				КурсДок=Док.Курс;
		КонецЕсли; 
			
		КолПрод=0;   // Для документа ПродажаЧерезКассу
		СумПрод=0;
		КолВозвр=0;
		СумВозвр=0;
		КолДок=0;
		
    	ЦенаДок=0; 
    	СуммаДок=0;   
    	ЦенаБаз=0; 
    	СуммаБаз=0; 
		
        Док.ВыбратьСтроки();
        Пока Док.ПолучитьСтроку()=1 Цикл
        	Если Док.Товар=ВыбТовар Тогда
				Попытка
					ЕдДок=Док.Единица;  
				Исключение   
					ЕдДок=ВыбТовар.БазоваяЕдиница;  
				КонецПопытки;
				Если ВидДок="ВводОстатковТоваров" Тогда   
