		Заг1=Заг1+"  Klients "+ВыбКлиент.Код+" "+ВыбКлиент.Наименование+"."; 
		ТекстЗапроса = ТекстЗапроса + "Условие (Клиент в ВыбКлиент);";
    КонецЕсли;

    Если ПустоеЗначение(ВыбОперация)=0 Тогда
    	Заг1=Заг1+"  tirdzniec®bas operўcija "+ВыбПризнакНакладной+".";
    КонецЕсли;  
    
    Если ВыбАгент.Выбран()=1 Тогда
		Заг1=Заг1+"  AЄents "+ВыбАгент.Наименование+"."; 
    КонецЕсли;
	    
	Если Запрос.Выполнить(ТекстЗапроса)=0 тогда
		Возврат;
	КонецЕсли;

	ВБ=Рубли;

 	ЧислоСтрок=0;
	//  Создание Таблицы для выходного отчета
	Таб=СоздатьОбъект("Таблица");
	Таб.ВывестиСекцию("Отчет");
	Оживить(4);
	Пока Запрос.Группировка("Док") = 1 цикл
        Док=Запрос.Док;   
		Если Док.Выбран()=0 Тогда
			Продолжить;
		КонецЕсли; 
		ВидДок=Док.Вид();
// Анализ условий диалога 		
		Если ВыбАгент.Выбран()=1 Тогда
			Если НЕ((ВидДок="РасходнаяНал") ИЛИ
