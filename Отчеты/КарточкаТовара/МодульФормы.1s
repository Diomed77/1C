	ТД.НоваяКолонка("Приход","Число",17,3);
	ТД.НоваяКолонка("Расход","Число",17,3);
	ТД.НоваяКолонка("Количество","Число",17,3);
	ТД.НоваяКолонка("Остаток","Число",17,3);
	ТД.НоваяКолонка("БазСто","Число",17,5);
	ТД.НоваяКолонка("ПродСто","Число",17,5);
	ТД.НоваяКолонка("Дилер","Справочник.Сотрудники");
	ТД.НоваяКолонка("Партнер");

	ВремРегистр=СоздатьОбъект("Регистры");
	Рег=ВремРегистр.ПартииТоваров;
	Рег.ВременныйРасчет();

	Если ВыбТовар.Выбран() = 0 Тогда
		Сообщить("Выберите товар!");
		Возврат;
	ИначеЕсли ВыбТовар.ЭтоГруппа() = 1 Тогда 
		Сообщить("Выберите конкретный товар,а не группу!");
		Возврат;
	КонецЕсли; 
	
	СписСтатусов=СоздатьОбъект("СписокЗначений");
	СписСтатусов.ДобавитьЗначение("");
	СписСтатусов.ДобавитьЗначение(Купленный);
	СписСтатусов.ДобавитьЗначение(Принятый);
	СписСтатусов.ДобавитьЗначение(Произведенный);
	
	Рег.УстановитьЗначениеФильтра("Статус",СписСтатусов,2);

	
	Рег.УстановитьЗначениеФильтра("Товар",ВыбТовар);
	
	Если (СрокНач=СрокКон) И (ПустоеЗначение(СрокКон)=0) Тогда
		Рег.УстановитьЗначениеФильтра("СрокРеализации",СрокНач);
	КонецЕсли;

	Если ВыбСклад.Выбран()=0 Тогда                      
    ИначеЕсли ВыбСклад.ЭтоГруппа()=1 Тогда
		Рег.УстановитьЗначениеФильтра("Склад",ВыбСклад,2);
	Иначе
		Рег.УстановитьЗначениеФильтра("Склад",ВыбСклад);
	КонецЕсли;	

	ВыбПризнакНакладной=ВыбОперация.ПолучитьЗначение(ВыбОперация.ТекущаяСтрока());
	Если ПустоеЗначение(ВыбПризнакНакладной) = 1 Тогда
	Иначе
    	Заг1=Заг1+"  tirdzniec®bas operўcija "+ВыбПризнакНакладной+".";
	КонецЕсли;
	
    Если ВыбАгент.Выбран()=1 Тогда
		Заг1=Заг1+"  AЄents "+ВыбАгент.Наименование+"."; 
    КонецЕсли;
	
	ВремРегистр.РассчитатьРегистрыНа(ДатаНачала);
	
	Если Датаконца=ПолучитьДатуТА() Тогда 
		Рег.ВыбратьДвиженияСОстатками(0)
	Иначе
		Рег.ВыбратьДвиженияСОстатками(ДатаКонца);
	КонецЕсли;	
              
	НС=0;
	ЗапросСКЛ=СоздатьОбъект("ODBCRecordSet");
	ТекстЗапроса="SELECT TOP 1 CASE WHEN $ОборотыТоваров.Количество =0 THEN $ОборотыТоваров.СуммаПЦЧ ELSE $ОборотыТоваров.СуммаПЦЧ/$ОборотыТоваров.Количество END Цена
	|			  FROM $Регистр.ОборотыТоваров AS ОборотыТоваров (NOLOCK)
	|			  WHERE (ОборотыТоваров.IDDOC = :Док)
	|					AND ($ОборотыТоваров.Товар = :Товар)
	|";
	
	Пока  (Рег.ПолучитьДвижение()>0) Цикл
		        
		текДок=Рег.ТекущийДокумент();
		
		Если глЕстьРеквизитШапки("КатегорияСписания",текДок.Вид())=1 Тогда 
			Попытка
				Партнер=Метаданные.Перечисление("КатегорииСписания").Значение(текДок.КатегорияСписания.Идентификатор()).Комментарий;
			Исключение
				Партнер=текДок.КатегорияСписания;
			КонецПопытки;
		ИначеЕсли глЕстьРеквизитШапки("Клиент",текДок.Вид())=1 Тогда
			Партнер=текДок.Клиент;
		Иначе
			Партнер=ПолучитьПустоеЗначение("Справочник.Контрагенты");
		КонецЕсли;
		
	    Если ВыбКлиент.Выбран()=0 Тогда
	    ИначеЕсли ВыбКлиент.ЭтоГруппа()=1 Тогда
			Если Партнер.ПринадлежитГруппе(ВыбКлиент)=0 Тогда
				Продолжить;
			КонецЕсли;
		Иначе
			Если Партнер<>ВыбКлиент Тогда
				Продолжить;
			КонецЕсли;
	    КонецЕсли;

		Если ПустоеЗначение(ВыбПризнакНакладной) = 0 Тогда
			попытка
				Если текДок.ПризнакНакладной<>ВыбПризнакНакладной Тогда
				    Продолжить;
				КонецЕсли;
			Исключение
				
			Конецпопытки;
		КонецЕсли;

	    Если ВыбАгент.Выбран()=1 Тогда
			попытка
				Если текДок.Агент<>ВыбАгент Тогда
				    Продолжить;
				КонецЕсли; 
			Исключение
				
			КонецПопытки;
	    КонецЕсли;
		
	    Если (ПустоеЗначение(СрокНач)=1) И (ПустоеЗначение(СрокКон)=1) Тогда
	    ИначеЕсли (Рег.СрокРеализации >= СрокНач) И (Рег.СрокРеализации <= СрокНач) Тогда
	    Иначе
			Продолжить;
	    КонецЕсли;
		
		Ост=Рег.СводныйОстаток(,ВыбТовар,,,,,,,"ОстатокТовара");

		Если Рег.Приход=1 Тогда
			Прих=Рег.ОстатокТовара;
			Расх=0;
		иначе      
			Прих=0;
			Расх=Рег.ОстатокТовара;
		КонецЕсли;               
		
		НС=НС+1;
		ТД.НоваяСтрока();
		ТД.НС=НС;
		ТД.Документ=текДок;
		ТД.Склад=Рег.Склад;
		ТД.СрокРеализ=Рег.СрокРеализации;
		ТД.ЭтоПриход=Рег.Приход;
		ТД.Приход=Прих;
		ТД.Расход=Расх;
		ТД.Количество=Рег.ОстатокТовара;
		ТД.Остаток=Ост;
		ТД.БазСто=Рег.Стоимость;
		
		ЗапросСКЛ.УстановитьТекстовыйПараметр("Док",текДок);
		ЗапросСКЛ.УстановитьТекстовыйПараметр("Товар",ВыбТовар);
		ТД.ПродСто  = ЗапросСКЛ.ВыполнитьСкалярный(ТекстЗапроса);
		
//		ТД.ПродСто=Пересчет(Рег.Оборот,ВыбТовар.ВалютаУчета,ТД.Документ.ДатаДок,Константа.ОсновнаяВалюта,ТД.Документ.ДатаДок);
		
		попытка
			ТД.Дилер=текДок.Агент;
		Исключение
		конецпопытки;
		ТД.Партнер=Партнер;
		
	КонецЦикла;

	ТД.Сортировать("-НС");	
	
	ТД.ВыбратьСтроки();
	Пока ТД.ПолучитьСтроку()=1 Цикл
        
		Док=ТД.Документ; 
		ПечДок=ПеревестиНаГосЯзык(Док.Вид())+" "+Док.ДатаДок+" Nr. "+СокрП(Док.НомерДок)+"; ";
		ДТ=док.ДатаДок;

		Таб.ВывестиСекцию("Товар");
		Оживить(1);
	    
	КонецЦикла;
	
	Таб.ВывестиСекцию("Итог");	
	
	//Вызов выходного отчета в окно просмотра и редактирования.
	
	Таб.Опции(0,0,0,0,ПарСтрОтчШирок);   
	Таб.параметрыСтраницы(2,100,1,2,2,5,5,2,2,1);
	
	Таб.Защита(Константа.ФлагЗащитыТаблиц);
	Таб.ТолькоПросмотр(1);
	Таб.Показать("Отчет об оборотах товаров","");
	семЗаписатьЛогКон( начЛог );	
КонецПроцедуры

Если ПустоеЗначение(ДатаОтчета)=1 Тогда
	ДатаОтчета='01.01.98';
